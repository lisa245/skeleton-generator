{
  "manifest_version": 3,
  "name": "skeleton-generator",
  "description": "Base Level Extension",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "extensions.png"
  },
  "permissions": ["contextMenus", "storage", "clipboardWrite"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "js": ["dist/content.js"],
      "matches": ["*://*/*"]
    }
  ]
}
